{
    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",

    "name": "toy",
    "db": "toy",
    "preprocessed": true,

    "tarfile": "toy_pre.tar.gz",

    "configure": "true",
    "make": "make CFLAGS+=-g",
    "clean": "make clean",
    "install": "make install",
    "command": "{install_dir}/bin/toy {input_file}",

    "max_liveness": 100000,
    "max_cardinality": 100,
    "max_tcn": 100000,
    "max_lval_size": 100,

    "main_file": [ "toy.c" ],

    "inputs": [ "inputs/testsmall.bin" ]
}
