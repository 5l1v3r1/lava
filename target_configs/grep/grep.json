{
    "name": "grep",
    "db": "grep",
    "preprocessed": true,
    "dataflow": true,

    "tarfile": "grep-3.1-pre.tar.gz",

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",


    "configure": "true",
    "make": "make CFLAGS=-gdwarf-2",
    "clean": "make clean",
    "install": "make install",
    "command": "{install_dir}/bin/grep '\\(.\\).\\(.\\)' {input_file}",
    "injfixupsscript": "fixup.sh {bug_build}",

    "max_liveness": 100000,
    "max_cardinality": 100,
    "max_tcn": 100000,
    "max_lval_size": 100,

    "curtail_fbi": 5000,

    "main_file": [ "grep-pre.c" ], 

    "inputs": [ "inputs/hi.txt" ]
}
