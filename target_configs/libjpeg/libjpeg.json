{
    "name": "libjpeg",
    "db": "libjpeg",
    "preprocessed": true,

    "tarfile": "libjpeg-9c-pre-m32.tgz",

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",


    "configure": "true",
