{
    "name": "tweetNaCl",
    "db": "tweetNaCl",
    "preprocessed": true,
    "dataflow": true,

    "tarfile": "tweetNaCl-pre.tar.gz",

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",


    "make": "make CFLAGS+=-fvisibility=default -j 16",
    "clean": "make clean",
    "install": "make install",
    "command": "{install_dir}/bin/tweetNaCl {input_file}",

    "max_liveness": 100000,
    "max_cardinality": 100,
    "max_tcn": 10,
    "max_lval_size": 100,

    "main_file": [ "main.c" ],

    "inputs": ["inputs/input.bin"]
}
